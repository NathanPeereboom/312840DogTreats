    <Window x:Class="_312840DogTreats.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_312840DogTreats"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Label>Small Treats</Label>
        <TextBox x:Name="txtSmall"/>
        <Label>Medium Treats</Label>
        <TextBox x:Name="txtMedium"/>
        <Label>Large Treats</Label>
        <TextBox x:Name="txtLarge"/>
        <Button x:Name="btnFeed" Click="btnFeed_Click">Feed Dog</Button>
        <Label x:Name="lblOutput"/>
    </StackPanel>
</Window>
